            <FlowStep.Next>
              <FlowStep x:Name="__ReferenceID0" sap2010:WorkflowViewState.IdRef="FlowStep_2">
                <local:Prompt BookmarkName="EnterGuess" DisplayName="等待用户键入数字" sap2010:WorkflowViewState.IdRef="Prompt_1">
